<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
        <context id="kotlin" targetRuntime="MyBatis3Kotlin">
            <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
            <plugin type="com.github.dkurata38.mybatis_generator_plugins.kotlin.NotNullFieldPlugin"/>
            <plugin type="com.github.dkurata38.mybatis_generator_plugins.kotlin.SeparateSubPackagesPlugin"/>
            <commentGenerator>
                <property name="suppressDate" value="true"/>
                <property name="addRemarkComments" value="false"/>
            </commentGenerator>
            <jdbcConnection driverClass="org.hsqldb.jdbc.JDBCDriver"
                            connectionURL="jdbc:hsqldb:mem:testdb"
                            userId="sa" password=""/>

            <javaTypeResolver>
                <property name="useJSR310Types" value="true"/>
            </javaTypeResolver>

            <javaModelGenerator
                    targetPackage="com.github.dkurata38.open_web_library.repository.mybatis3_kotlin"
                    targetProject="/Users/kuratadaisuke/Projects/open-web-library/source/open-web-library/generator/src/main/kotlin">
                <property name="enableSubPackages" value="true"/>
            </javaModelGenerator>
            <javaClientGenerator
                    targetPackage="com.github.dkurata38.open_web_library.repository.mybatis3_kotlin"
                    targetProject="/Users/kuratadaisuke/Projects/open-web-library/source/open-web-library/generator/src/main/kotlin">
                <property name="enableSubPackages" value="true"/>
            </javaClientGenerator>

            <table tableName="member"/>
        </context>
        <context id="MyBatis3DynamicSql" targetRuntime="MyBatis3DynamicSql">
            <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
            <!--        <plugin type="com.github.dkurata38.mybatis_generator_plugins.kotlin.NotNullFieldPlugin"/>-->
            <plugin type="com.github.dkurata38.mybatis_generator_plugins.kotlin.SeparateSubPackagesPlugin"/>
            <commentGenerator>
                <property name="suppressDate" value="true"/>
                <property name="addRemarkComments" value="false"/>
            </commentGenerator>
            <jdbcConnection driverClass="org.hsqldb.jdbc.JDBCDriver"
                            connectionURL="jdbc:hsqldb:mem:testdb"
                            userId="sa" password=""/>

            <javaTypeResolver>
                <property name="useJSR310Types" value="true"/>
            </javaTypeResolver>

            <javaModelGenerator
                    targetPackage="com.github.dkurata38.open_web_library.repository.mybatis3_dsql"
                    targetProject="/Users/kuratadaisuke/Projects/open-web-library/source/open-web-library/generator/src/main/java">
                <property name="enableSubPackages" value="true"/>
            </javaModelGenerator>
            <javaClientGenerator
                    targetPackage="com.github.dkurata38.open_web_library.repository.mybatis3_dsql"
                    targetProject="/Users/kuratadaisuke/Projects/open-web-library/source/open-web-library/generator/src/main/java">
                <property name="enableSubPackages" value="true"/>
            </javaClientGenerator>

            <table tableName="member"/>
        </context>
    <context id="MyBatis3" targetRuntime="MyBatis3">
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
        <!--        <plugin type="com.github.dkurata38.mybatis_generator_plugins.kotlin.NotNullFieldPlugin"/>-->
        <!--        <plugin type="com.github.dkurata38.mybatis_generator_plugins.SeparateSubPackagesPlugin"/>-->
        <plugin type="com.github.dkurata38.mybatis_generator_plugins.mybatis3.CustomAliasedColumnListPlugin"/>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="false"/>
        </commentGenerator>
        <jdbcConnection driverClass="org.hsqldb.jdbc.JDBCDriver"
                        connectionURL="jdbc:hsqldb:mem:testdb"
                        userId="sa" password=""/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator
                targetPackage="com.github.dkurata38.mybatis_generator_plugins.mybatis_generator_plugins_test.mybatis3"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator
                targetPackage="com.github.dkurata38.mybatis_generator_plugins.mybatis_generator_plugins_test.mybatis3"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator
                targetPackage="com.github.dkurata38.mybatis_generator_plugins.mybatis_generator_plugins_test.mybatis3"
                targetProject="MAVEN"
                type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="member"/>
    </context>
</generatorConfiguration>
