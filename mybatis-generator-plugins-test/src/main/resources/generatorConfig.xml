<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="MyBatis3KotlinNotNull" targetRuntime="MyBatis3Kotlin">
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
        <plugin type="com.github.kuchita_el.mybatis_generator_plugins.kotlin.NotNullFieldPlugin"/>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="false"/>
        </commentGenerator>
        <jdbcConnection driverClass="${mybatis.generator.jdbcConnection.driverClass}"
                        connectionURL="${mybatis.generator.jdbcConnection.connectionURL}"
                        userId="${mybatis.generator.jdbcConnection.userId}" password=""/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.not_null_field.mybatis3_kotlin"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>
        <javaClientGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.not_null_field.mybatis3_kotlin"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="member"/>
    </context>

    <context id="MyBatis3KotlinValField" targetRuntime="MyBatis3Kotlin">
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
        <plugin type="com.github.kuchita_el.mybatis_generator_plugins.kotlin.ValFieldPlugin"/>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="false"/>
        </commentGenerator>
        <jdbcConnection driverClass="${mybatis.generator.jdbcConnection.driverClass}"
                        connectionURL="${mybatis.generator.jdbcConnection.connectionURL}"
                        userId="${mybatis.generator.jdbcConnection.userId}" password=""/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.val_field.mybatis3_kotlin"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>
        <javaClientGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.val_field.mybatis3_kotlin"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="member"/>
    </context>

    <context id="MyBatis3DynamicSql" targetRuntime="MyBatis3DynamicSql">
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
        <jdbcConnection driverClass="${mybatis.generator.jdbcConnection.driverClass}"
                        connectionURL="${mybatis.generator.jdbcConnection.connectionURL}"
                        userId="${mybatis.generator.jdbcConnection.userId}" password=""/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.mybatis3_dsql"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>
        <javaClientGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.mybatis3_dsql"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="member"/>
    </context>

    <context id="MyBatis3SelectForUpdateXML" targetRuntime="MyBatis3">
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>
        <plugin type="com.github.kuchita_el.mybatis_generator_plugins.SelectForUpdatePlugin"/>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="false"/>
        </commentGenerator>
        <jdbcConnection driverClass="${mybatis.generator.jdbcConnection.driverClass}"
                        connectionURL="${mybatis.generator.jdbcConnection.connectionURL}"
                        userId="${mybatis.generator.jdbcConnection.userId}" password=""/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.select_for_update.mybatis3.xml"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.select_for_update..mybatis3.xml"
                targetProject="MAVEN">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator
                targetPackage="com.github.kuchita_el.mybatis_generator_plugins.mybatis_generator_plugins_test.select_for_update.mybatis3.xml"
                targetProject="MAVEN"
                type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="member"/>
    </context>
</generatorConfiguration>
